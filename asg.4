void main() {
  
  var array = [1,23,44,55,21,10];
  int max = array[1];
  int min = array[6];
  for(int i =0; i<array.length; i++){
    if(array[i]>max){
      max = array[i];
    }
    else if (array[i]<min){
      min=array[i];
    }
  }
  print("maximum value is : $max");
  print("minimum value is : $min");
}  